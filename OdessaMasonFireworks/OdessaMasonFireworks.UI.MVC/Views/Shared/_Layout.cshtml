<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <title>@ViewData["Title"] | Odessa Masons</title>
    <!-- Favicon-->
    <link rel="icon" type="image/x-icon" href="~/assets/573-5737790_masonic-square-and-compasses-square-and-compass.png" />
    <!-- Bootstrap icons-->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css" rel="stylesheet" />
    <!-- Core theme CSS (includes Bootstrap)-->
    <link href="~/css/styles.css" rel="stylesheet" />
    <link href="~/css/Custom.css" rel="stylesheet"/>
    <!-- Datatables Styling-->
    <link href="https://cdn.datatables.net/1.13.4/css/jquery.dataTables.min.css" />
    <!-- DataTables Buttons Style-->
    <link href="https://cdn.datatables.net/1.13.4/css/jquery.dataTables.min.css" rel="stylesheet" />
    <link href="https://cdn.datatables.net/buttons/1.5.1/css/buttons.dataTables.min.css" rel="stylesheet" />
</head>
<body class="d-flex flex-column h-100">
    <main class="flex-shrink-0">
        <!-- Navigation-->
        <nav class="navbar navbar-expand-lg navbar-dark bg-dark custom-blue">
            <div class="container px-5 ">
                <a class="navbar-brand custom-gold" href="index.html">Mount Hope Masonic Lodge #476</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation"><span class="navbar-toggler-icon"></span></button>
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav ms-auto mb-2 mb-lg-0">
                        <li class="nav-item"><a class="nav-link custom-gold" href="index.html">Home</a></li>
                        <li class="nav-item"><a class="nav-link custom-gold" href="about.html">About</a></li>
                        <li class="nav-item"><a class="nav-link custom-gold" href="contact.html">Contact</a></li>
                        <li class="nav-item"><a class="nav-link custom-gold" href="faq.html">FAQ</a></li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle custom-gold" id="navbarDropdownPortfolio" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false">Fireworks</a>
                            <ul class="dropdown-menu dropdown-menu-end" aria-labelledby="navbarDropdownPortfolio">
                                <a class="nav-link text-bg-danger" asp-action="Index" asp-controller="Brands">Brands</a>
                                <a class="nav-link text-bg-danger" asp-action="Index" asp-controller="Customers">Customers</a>
                                <a class="nav-link text-bg-danger" asp-action="Index" asp-controller="Members">Members</a>
                                <a class="nav-link text-bg-danger" asp-action="Index" asp-controller="Orders">Orders</a>
                                <a class="nav-link text-bg-danger" asp-action="Index" asp-controller="OrderProducts">Order Products</a>
                                <a class="nav-link text-bg-danger" asp-action="Index" asp-controller="Fireworks">Fireworks</a>
                                <a class="nav-link text-bg-danger" asp-action="Index" asp-controller="ProductTypes">Product Typess</a>
                                <a class="nav-link text-bg-danger" asp-action="Index" asp-controller="Sales">Sales</a>
                                <a class="nav-link text-bg-danger" asp-action="Index" asp-controller="SaleProducts">SaleProducts</a>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
        @RenderBody()
    </main>
    <!-- Footer-->
    <footer class="bg-dark py-4 mt-auto custom-blue custom-gold">
        <div class="container px-5">
            <div class="row align-items-center justify-content-between flex-column flex-sm-row">
                <div class="col-auto"><div class="small m-0">Copyright &copy; Mount Hope Masonic Lodge #476 (@DateTime.Now.Year)</div></div>
                <div class="col-auto">
                    <a class="link-light small" href="#!">Privacy</a>
                    <span class="text-white mx-1">&middot;</span>
                    <a class="link-light small" href="#!">Terms</a>
                    <span class="text-white mx-1">&middot;</span>
                    <a class="link-light small" href="#!">Contact</a>
                </div>
            </div>
        </div>
    </footer>
    <!-- Bootstrap core JS-->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>
    <!-- Core theme JS-->
    <script src="~/js/scripts.js"></script>
    <script src="~/js/jquery.js" ></script>
    <script src="https://cdn.datatables.net/1.13.4/js/jquery.dataTables.min.js"></script>
    @RenderSection("Scripts", false)
</body>
</html>
